{
  "name": "kypo-adaptive-model-simulator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint && npm run prettier-check",
    "e2e": "ng e2e",
    "ci-test": "ng test --browsers ChromeHeadless --watch=false",
    "prettier-check": "prettier --check \"projects/**/*.ts\"",
    "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
    "build-example-app": "ng build kypo-adaptive-model-simulator-example --configuration production",
    "build-library": "ng build --configuration production kypo-adaptive-model-simulator",
    "create-package": "cd dist/kypo-adaptive-model-simulator && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "ci-update-version": "cd projects/kypo-adaptive-model-simulator/ && npm version $TAG_VERSION",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/kypo-adaptive-model-simulator && npm publish",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/builder": "~13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/animations": "~13.1.2",
    "@angular/cdk": "^13.1.2",
    "@angular/common": "~13.1.2",
    "@angular/compiler": "~13.1.2",
    "@angular/core": "~13.1.2",
    "@angular/forms": "~13.1.2",
    "@angular/material": "^13.1.2",
    "@angular/platform-browser": "~13.1.2",
    "@angular/platform-browser-dynamic": "~13.1.2",
    "@angular/router": "~13.1.2",
    "@muni-kypo-crp/adaptive-transition-visualization": "13.0.0",
    "@muni-kypo-crp/adaptive-visualization": "~13.1.0",
    "@muni-kypo-crp/d3-service": "12.0.2",
    "@muni-kypo-crp/training-api": "~13.0.7",
    "@muni-kypo-crp/training-model": "~13.0.6",
    "@sentinel/auth": "~13.0.0",
    "@sentinel/common": "~13.0.1",
    "@sentinel/components": "~13.3.0",
    "@sentinel/layout": "~13.0.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "angular-oauth2-oidc": "^13.0.1",
    "angular-oauth2-oidc-jwks": "^13.0.1",
    "d3": "^7.1.1",
    "d3-sankey": "^0.12.3",
    "eslint": "^8.6.0",
    "file-saver": "2.0.5",
    "ngx-indexed-db": "^9.4.2",
    "rxjs": "~7.5.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "~13.1.2",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^17.0.8",
    "@types/d3": "^7.1.0",
    "husky": "^8.0.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^13.1.3",
    "prettier": "^2.5.1",
    "typescript": "~4.5.5"
  }
}
