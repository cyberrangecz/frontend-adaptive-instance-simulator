{
  "name": "kypo-adaptive-model-simulator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint && npm run prettier-check",
    "e2e": "ng e2e",
    "ci-test": "ng test --browsers ChromeHeadless --watch=false",
    "prettier-check": "prettier --check \"projects/**/*.ts\"",
    "prettier-fix": "prettier --write \"./projects/**/*.ts\"",
    "build-example-app": "ng build kypo-adaptive-model-simulator-example --configuration production",
    "build-library": "ng build --configuration production kypo-adaptive-model-simulator",
    "create-package": "cd dist/kypo-adaptive-model-simulator && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "ci-update-version": "cd projects/kypo-adaptive-model-simulator/ && npm version $TAG_VERSION",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/kypo-adaptive-model-simulator && npm publish",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/builder": "~14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/schematics": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/animations": "^14.0.3",
    "@angular/cdk": "^14.0.3",
    "@angular/common": "^14.0.3",
    "@angular/compiler": "^14.0.3",
    "@angular/core": "^14.0.3",
    "@angular/forms": "^14.0.3",
    "@angular/material": "^14.0.3",
    "@angular/platform-browser": "^14.0.3",
    "@angular/platform-browser-dynamic": "^14.0.3",
    "@angular/router": "^14.0.3",
    "@muni-kypo-crp/adaptive-transition-visualization": "14.0.1",
    "@muni-kypo-crp/adaptive-visualization": "~14.0.0",
    "@muni-kypo-crp/d3-service": "14.0.0",
    "@muni-kypo-crp/training-api": "~14.0.0",
    "@muni-kypo-crp/training-model": "~14.0.0",
    "@sentinel/auth": "~14.0.0",
    "@sentinel/common": "~14.0.0",
    "@sentinel/components": "~14.0.1",
    "@sentinel/layout": "~14.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "angular-oauth2-oidc": "^13.0.1",
    "angular-oauth2-oidc-jwks": "^13.0.1",
    "caret-pos": "^2.0.0",
    "d3": "^7.4.4",
    "d3-sankey": "^0.12.3",
    "eslint": "^8.18.0",
    "file-saver": "2.0.5",
    "marked": "^4.0.17",
    "ngx-indexed-db": "^11.0.2",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular/cli": "^14.0.3",
    "@angular/compiler-cli": "^14.0.3",
    "@types/d3": "^7.4.0",
    "@types/jasmine": "~4.0.3",
    "@types/marked": "^4.0.3",
    "@types/node": "^18.0.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.0.2",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4"
  }
}
