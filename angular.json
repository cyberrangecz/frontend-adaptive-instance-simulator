{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "adaptive-model-simulator": {
            "projectType": "library",
            "root": "projects/adaptive-model-simulator",
            "sourceRoot": "projects/adaptive-model-simulator",
            "prefix": "crczp",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/adaptive-model-simulator/tsconfig.lib.json",
                        "project": "projects/adaptive-model-simulator/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/adaptive-model-simulator/tsconfig.lib.prod.json"
                        },
                        "development": {}
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/adaptive-model-simulator/test.ts",
                        "tsConfig": "projects/adaptive-model-simulator/tsconfig.spec.json",
                        "karmaConfig": "projects/adaptive-model-simulator/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "projects/adaptive-model-simulator/**/*.ts",
                            "projects/adaptive-model-simulator/**/*.html"
                        ]
                    }
                }
            }
        },
        "adaptive-model-simulator-example-app": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "root": "projects/adaptive-model-simulator-example-app",
            "sourceRoot": "projects/adaptive-model-simulator-example-app/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/adaptive-model-simulator-example-app",
                        "index": "projects/adaptive-model-simulator-example-app/src/index.html",
                        "main": "projects/adaptive-model-simulator-example-app/src/main.ts",
                        "polyfills": "projects/adaptive-model-simulator-example-app/src/polyfills.ts",
                        "tsConfig": "projects/adaptive-model-simulator-example-app/tsconfig.app.json",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "node_modules/@cyberrangecz-platform/theme/assets",
                                "output": "assets/"
                            }
                        ],
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "node_modules/@cyberrangecz-platform/theme/"
                            ]
                        },
                        "styles": [
                            "node_modules/@cyberrangecz-platform/theme/theming/apply-all.scss",
                            "projects/adaptive-model-simulator-example-app/src/styles.scss"
                        ],
                        "scripts": [],
                        "vendorChunk": true,
                        "extractLicenses": false,
                        "buildOptimizer": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/adaptive-model-simulator-example-app/src/environments/environment.ts",
                                    "with": "projects/adaptive-model-simulator-example-app/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "local": {
                            "budgets": [
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "projects/adaptive-model-simulator-example-app/src/environments/environment.ts",
                                    "with": "projects/adaptive-model-simulator-example-app/src/environments/environment.local.ts"
                                }
                            ]
                        }
                    },
                    "defaultConfiguration": ""
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "buildTarget": "adaptive-model-simulator-example-app:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "adaptive-model-simulator-example-app:build:production"
                        },
                        "local": {
                            "buildTarget": "adaptive-model-simulator-example-app:build:local"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "adaptive-model-simulator-example-app:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/adaptive-model-simulator-example-app/src/test.ts",
                        "polyfills": "projects/adaptive-model-simulator-example-app/src/polyfills.ts",
                        "tsConfig": "projects/adaptive-model-simulator-example-app/tsconfig.spec.json",
                        "karmaConfig": "projects/adaptive-model-simulator-example-app/karma.conf.js",
                        "assets": [
                            "projects/adaptive-model-simulator-example-app/src/favicon.ico",
                            "projects/adaptive-model-simulator-example-app/src/assets"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "projects/adaptive-model-simulator-example-app/src/styles.scss"
                        ],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": [
                            "projects/adaptive-model-simulator-example-app/src/app/visualizations/**/*.ts",
                            "projects/adaptive-model-simulator-example-app/**/*.component.html"
                        ]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "adaptive-model-simulator-example-app:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "adaptive-model-simulator-example-app:serve:production"
                        }
                    }
                }
            }
        }
    },
    "schematics": {
        "@angular-eslint/schematics:application": {
            "setParserOptionsProject": true
        },
        "@angular-eslint/schematics:library": {
            "setParserOptionsProject": true
        }
    },
    "cli": {
        "analytics": false
    }
}
