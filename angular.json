{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "kypo-adaptive-model-simulator": {
      "projectType": "library",
      "root": "projects/kypo-adaptive-model-simulator",
      "sourceRoot": "projects/kypo-adaptive-model-simulator/src",
      "prefix": "kypo-adaptive-model-simulator",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/kypo-adaptive-model-simulator/tsconfig.lib.json",
            "project": "projects/kypo-adaptive-model-simulator/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/kypo-adaptive-model-simulator/tsconfig.lib.prod.json"
            },
            "development": {}
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kypo-adaptive-model-simulator/test.ts",
            "tsConfig": "projects/kypo-adaptive-model-simulator/tsconfig.spec.json",
            "karmaConfig": "projects/kypo-adaptive-model-simulator/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/kypo-adaptive-model-simulator/**/*.ts",
              "projects/kypo-adaptive-model-simulator/**/*.html"
            ]
          }
        }
      }
    },
    "kypo-adaptive-model-simulator-example": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/kypo-adaptive-model-simulator-example",
      "sourceRoot": "projects/kypo-adaptive-model-simulator-example/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/kypo-adaptive-model-simulator-example",
            "index": "projects/kypo-adaptive-model-simulator-example/src/index.html",
            "main": "projects/kypo-adaptive-model-simulator-example/src/main.ts",
            "polyfills": "projects/kypo-adaptive-model-simulator-example/src/polyfills.ts",
            "tsConfig": "projects/kypo-adaptive-model-simulator-example/tsconfig.app.json",
            "assets": [
              "projects/kypo-adaptive-model-simulator-example/src/favicon.ico",
              "projects/kypo-adaptive-model-simulator-example/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/kypo-adaptive-model-simulator-example/src/styles.scss"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/kypo-adaptive-model-simulator-example/src/environments/environment.ts",
                  "with": "projects/kypo-adaptive-model-simulator-example/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "local": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/kypo-adaptive-model-simulator-example/src/environments/environment.ts",
                  "with": "projects/kypo-adaptive-model-simulator-example/src/environments/environment.local.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kypo-adaptive-model-simulator-example:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "kypo-adaptive-model-simulator-example:build:production"
            },
            "local": {
              "browserTarget": "kypo-adaptive-model-simulator-example:build:local"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kypo-adaptive-model-simulator-example:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kypo-adaptive-model-simulator-example/src/test.ts",
            "polyfills": "projects/kypo-adaptive-model-simulator-example/src/polyfills.ts",
            "tsConfig": "projects/kypo-adaptive-model-simulator-example/tsconfig.spec.json",
            "karmaConfig": "projects/kypo-adaptive-model-simulator-example/karma.conf.js",
            "assets": [
              "projects/kypo-adaptive-model-simulator-example/src/favicon.ico",
              "projects/kypo-adaptive-model-simulator-example/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/kypo-adaptive-model-simulator-example/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/kypo-adaptive-model-simulator-example/src/app/visualizations/**/*.ts",
              "projects/kypo-adaptive-model-simulator-example/**/*.component.html"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "kypo-adaptive-model-simulator-example:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "kypo-adaptive-model-simulator-example:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "kypo-adaptive-model-simulator"
}
